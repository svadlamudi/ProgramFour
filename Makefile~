##Start of Makefile##
prog4: main.o line_list.o bin_tree_mod.o
	g++ -o prog4 main.o line_list.o bin_tree_mod.o
	make clean
main.o: main.cpp global_vars.h
	g++ -Wall -c -g main.cpp
line_list.o: line_list.cpp global_vars.h
	g++ -Wall -c -g line_list.cpp
bin_tree_mod.o: bin_tree_mod.cpp global_vars.h
	g++ -Wall -c -g bin_tree_mod.cpp
clean:
	rm *.o
##End of Makefile##